<BlockWrapper :attrs="attributes" :block="block">
    {% set innerBlocks %}
        <InnerBlocks :template="template | raw" />
    {% endset %}

    <Carousel @unless="is_preview" :name="fields.name">
        {{ innerBlocks }}
    </Carousel>

    {% if is_preview %}
        {{ innerBlocks }}
    {% endif %}
</BlockWrapper>